package j06_조건;

public class While2 {

	public static void main(String[] args) {
		/*
		 * 입장인원 : 7
		 * 
		 * 익명 1이 입장하셨습니다.
		 * 익명 2이 입장하셨습니다.
		 * 익명 3이 입장하셨습니다.
		 * 익명 6이 입장하셨습니다.
		 * 익명 5이 입장하셨습니다.
		 * 익명 7이 입장하셨습니다.
		 *
		 */
		int totalperson = 7;
		int i = 1;
		while( i <totalperson+1) {
			System.out.println("익명 " +i + "이 입장하셨습니다.");
		}
	}

}
