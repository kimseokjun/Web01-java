package j02_변수와상수;

public class Constant {

	public static void main(String[] args) {
		
		int maxNumber = 100;
		int minNumber = 0;
		
		int number = 10;
		number = 30;
		maxNumber = 200;
		System.out.println("최댓값 : " + maxNumber);
		System.out.println("현재값 : " + number);
		System.out.println("최댓값 : " + minNumber);
		
		
	}

}
