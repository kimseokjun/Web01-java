package j07_반복;

public class DoubleWhile2 {

	public static void main(String[] args) {
		int i = 2;
		
		while(i< 9)
		{
			int j = 1;
			System.out.println();
			System.out.println(i+1 + "단");
			System.out.println();
			while(j < 9)
			{
				System.out.println(i + " x " + (j+1) + " = " + i*(j+1) );
				j++;
			}
			i++;
		}
	}

}
