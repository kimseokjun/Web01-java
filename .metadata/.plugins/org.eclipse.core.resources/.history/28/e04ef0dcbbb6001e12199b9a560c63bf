package j01_출력;

public class Myinfo {

	public static void main(String[] args) {
		int x = 1;	//1 을 x에 대입한다.
		// int String double float char boolean
		// 변수는 메모리에 저장된다.
		
		char name1 = '홍';
		char name2 = '길';
		char name3 = '동';
		int age = 18;
		boolean result = true;
		
		String address = "부산 진구 가야동";	//문자는 '', 문자열""
		// String은 기본 자료형이 아니다. 그래서 색깔이 다르다.
		
		System.out.println(name1 + name2);	
		
		
	}

}
